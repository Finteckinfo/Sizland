# WalletConnect Cloud Project ID
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Clerk Authentication Publishable Key
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Your PostgreSQL database connection string
DATABASE_URL=your_database_url_here

# Your Stripe secret key (starts with sk_)
STRIPE_SECRET_KEY=your_stripe_secret_here

# Your Stripe webhook endpoint secret (starts with whsec_)
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Your Stripe publishable key (starts with pk_)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Algorand network URL (e.g., https://mainnet-api.algonode.cloud)
ALGORAND_NETWORK_URL=https://testnet-api.algonode.cloud

# Algorand network token (usually empty for public networks)
ALGORAND_NETWORK_TOKEN=

# SIZ token asset ID (e.g., 3186560531)
SIZ_TOKEN_ASSET_ID=your_siz_token_asset_id_here

# Central wallet address for token distribution
CENTRAL_WALLET_ADDRESS=your_central_wallet_address_here

# Central wallet mnemonic phrase (25 words)
CENTRAL_WALLET_MNEMONIC=your_central_wallet_mnemonic_here

# ARC-0059 router application ID
ARC59_APP_ID=your_arc59_app_id_here

# Freeze manager account address
UNFREEZE_ACCOUNT_ADDRESS=your_unfreeze_account_address_here

# Freeze manager account mnemonic phrase (25 words)
UNFREEZE_ACCOUNT_MNEMONIC=your_unfreeze_account_mnemonic_here

# NextAuth.js Configuration (for SSO)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Backend API URL
NEXT_PUBLIC_BACKEND_URL=http://localhost:5000

# Database SSL setting (set to "disable" if needed)
DB_SSL=your_value_here

# Test receiver address
TEST_RECEIVER_ADDRESS=

# Test receiver mnemonic
TEST_RECEIVER_MNEMONIC=

# Algod URL
ALGOD_URL=